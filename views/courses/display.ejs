<%- include('../header.ejs')  %>

<div class="container mb-5 mt-5">
    <table class="table table-bordered">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone No.</th>
            <th>Action</th>
        </tr>

        <% d.forEach((item) =>{ %>
            <tr>
                <td> <%= item._id %> </td>
                <td> <%= item.name %> </td>
                <td> <%= item.email %> </td>
                <td> <%= item.phone %> </td>
                <td>
                    <a href="/view/<%= item._id %>" class="btn btn-outline-info">view</a>
                    <a href="/edit/<%= item._id %>" class="btn btn-outline-success">edit</a>
                    <a href="/delete/<%= item._id %>" class="btn btn-outline-danger">delete</a>
                </td>
            </tr>
        <% }) %>
    </table>
</div>

<%- include('../footer.ejs')  %>